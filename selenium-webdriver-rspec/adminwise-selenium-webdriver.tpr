<?xml version="1.1" encoding="US-ASCII"?>
<project>
  <name>AdminWise-Selenium2</name>
  <default_browser>Firefox</default_browser>
  <framework>Selenium2-RSpec</framework>
  <environments>
    <entry default="false" name="build_server">
      <base_url>http://adminwise.heroku.com</base_url>
    </entry>
    <entry default="false" name="dev">
      <base_url>http://10.0.0.3:3000</base_url>
    </entry>
    <entry default="false" name="development">
      <base_url>http://localhost:3200</base_url>
    </entry>
    <entry default="false" name="special">
      <base_url>http://adminwise.macmini</base_url>
    </entry>
    <entry default="false" name="test">
      <base_url>http://adminwise.agileway.net</base_url>
    </entry>
  </environments>
  <exclude_dirs></exclude_dirs>
  <exclude_files></exclude_files>
  <test_named_as>test_suite</test_named_as>
  <spec_template></spec_template>
  <page_template>&lt;%if @info[:framework] &amp;&amp; @info[:framework] =~ /selenium/i %&gt;require 'rwebspec-webdriver'&lt;% else %&gt;require 'rwebspec'&lt;% end %&gt;

class &lt;%= @info[:page_class]%&gt; &lt; AbstractPage

  def initialize(browser)
    super(browser, "&lt;%= @info[:page_text] %&gt;") # &lt;= TEXT UNIQUE TO THIS PAGE
  end

&lt;%= @info[:page_method] rescue nil %&gt;
end</page_template>
  <storywise_url>http://storywise.agileway.net</storywise_url>
  <jira_url>http://jira.atlassian.com</jira_url>
  <jira_project>DEMO</jira_project>
  <jira_login>soaptester</jira_login>
  <jira_password>soaptester</jira_password>
  <heart_beat>
    <spec_suite></spec_suite>
    <environment></environment>
    <start_time></start_time>
    <repeat></repeat>
    <end_repeat></end_repeat>
    <emails></emails>
  </heart_beat>
</project>
